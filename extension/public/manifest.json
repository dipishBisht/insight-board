{
    "manifest_version": 3,
    "name": "InsightBoard",
    "version": "1.0.0",
    "description": "Track time spent on different websites",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "idle"
    ],
    "background": {
        "service_worker": "assets/background.js"
    }
}